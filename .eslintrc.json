{
  "extends": ["next/core-web-vitals", "eslint:recommended"],
  "rules": {
    "@next/next/no-html-link-for-pages": "off",
    "no-unused-vars": "warn",
    "no-console": ["warn", { "allow": ["warn", "error"] }]
  },
  "parserOptions": {
    "ecmaVersion": 2024,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es2024": true,
    "browser": true,
    "node": true
  }
}
